{
  "name": "ecommerce",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm run start",
    "server-install": "npm install",
    "client-install": "cd client && npm install",
    "install-all": "concurrently \"npm run server-install\"  \"npm run client-install\"",
    "dev": "concurrently \"npm run server\"  \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "argon2": "^0.27.2",
    "cloudinary": "^1.25.2",
    "concurrently": "^6.4.0",
    "config": "^3.3.6",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dateformat": "^4.5.1",
    "debug": "~2.6.9",
    "dotenv": "^9.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-fileupload": "^1.2.1",
    "googleapis": "^73.0.0",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mongoose": "^5.12.7",
    "morgan": "^1.9.1",
    "nodemailer": "^6.6.0",
    "nodemon": "^2.0.7",
    "public-ip": "^4.0.4",
    "qs": "^6.10.1",
    "serve-favicon": "^2.5.0",
    "sha256": "^0.2.0"
  }
}
